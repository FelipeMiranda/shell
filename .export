#!/bin/bash

OSTYPE=`uname`

HISTFILE=~/.bash_history
HISTSIZE=2000
VISUAL=vim
EDITOR=vim
PS1="\u@\h \w> "



export HISTFILE HISTSIZE PS1 EDITOR VISUAL


#CUSTOM OS
if [ $OSTYPE == "OpenBSD" ]; then
PATH=$PATH:~/shell/openbsd
PKG_PATH=http://mirrors.unb.br/pub/OpenBSD/5.7/packages/i386/
export PKG_PATH PATH

fi
