#!/bin/bash

function exibe_versao(){

	DATA=`cat $CDSHELL/backup/data_ultima_instalacao.txt`
	echo -en "\n$DATA\n"
}


case $1 in
   "install") cd $CDSHELL ; . install.sh
         ;;
   "-v") echo "Vers√£o 1.0.0"
         ;;
   "--help") echo "Digite miniman cdshell"
	 ;;
   *) exibe_versao
      exit 1
      ;;
esac


